{
  "markdown-pdf.highlightStyle": "solarized-dark.css",
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.formatOnSave": true,
  },
  // 抑止設定
  "markdownlint.config": {
    "MD010": false, // タブ
    "MD013": false // 長い行
  },
  // Paste Imageで保存時にファイル名を指定できるようにする
  "pasteImage.showFilePathConfirmInputBox": true,
}
